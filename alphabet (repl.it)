str = "aбвгдеёжзийклмнопрстуфхцчшщьыъэюя"
array = list(str)
array.reverse()
for i in range(len(array)):
  if array[i] == "а":
    array[i] = "я"
  elif array[i] == "б":
    array[i] = "п"
  elif array[i] == "в":
    array[i] = "ф"
  elif array[i] == "г":
    array[i] = "к"
  elif array[i] == "д":
    array[i] = "т"
  elif array[i] == "е":
    array[i] = "э"
  elif array[i] == "ж":
    array[i] = "ш"
  elif array[i] == "з":
    array[i] = "с"
  elif array[i] == "и":
    array[i] = "ы"
  elif array[i] == "к":
    array[i] = "г"
  elif array[i] == "о":
    array[i] = "ё"
  elif array[i] == "п":
    array[i] = "б"
  elif array[i] == "с":
    array[i] = "з"
  elif array[i] == "т":
    array[i] = "д"
  elif array[i] == "у":
    array[i] = "ю"
  elif array[i] == "ф":
    array[i] = "в"  
  elif array[i] == "ч":
    array[i] = "щ"
  elif array[i] == "ш":
    array[i] = "ж"
  elif array[i] == "щ":
    array[i] = "ч"
  elif array[i] == "ы":
    array[i] = "и"
  elif array[i] == "э":
    array[i] = "е"
  elif array[i] == "ю":
    array[i] = "у"
  elif array[i] == "я":
    array[i] = "а"
str = ''.join(array)
print(str)
